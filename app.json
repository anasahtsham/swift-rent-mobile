{
  "expo": {
    "name": "Swift Rent",
    "slug": "swift-rent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/favicons/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "userInterfaceStyle": "dark",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/favicons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/icons/favicons/favicon.png"
    }
  }
}